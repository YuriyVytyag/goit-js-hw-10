{{#each this}}
    <li class="list-item">
    <p>{{name.official}}</p>
    <img 
    src='{{flags.svg}}' 
    alt='{{name.official}}' 
    width="100"
    height="100"
    />
    <p>столиця: {{capital}}</p>
    <p>населення {{population}}</p>
    {{#each languages}}<p>мови:{{this}}</p>{{/each}}
    </li>
{{/each}}